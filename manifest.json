{
    "manifest_version" : 2,

    "name": "Zoomer",
    "description": "Save / Auto-open Zoom Links",
    "version": "0.1",


    "browser_action": {"default_popup": "popup.html","default_title":"Ok Zoomer"},

    "permissions": ["tabs", "storage"], 

    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

    "browser_action": {"default_popup" : "popup.html"},

    "background": {
    "scripts": ["background.js"],
    "persistent": false
    }
}